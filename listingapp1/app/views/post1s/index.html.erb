<p id="notice"><%= notice %></p>

<h1>Post1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Genere</th>
      <th>String</th>
      <th>Description</th>
      <th>String</th>
      <th>No.of</th>
      <th>Pages</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post1s.each do |post1| %>
      <tr>
        <td><%= post1.title %></td>
        <td><%= post1.genere %></td>
        <td><%= post1.string %></td>
        <td><%= post1.description %></td>
        <td><%= post1.string %></td>
        <td><%= post1.no.of %></td>
        <td><%= post1.pages %></td>
        <td><%= post1.integer %></td>
        <td><%= link_to 'Show', post1 %></td>
        <td><%= link_to 'Edit', edit_post1_path(post1) %></td>
        <td><%= link_to 'Destroy', post1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post1', new_post1_path %>
